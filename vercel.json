{
    "version": 2,
    "builds": [
        {
            "src": "./client/build",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/server"
        }
    ]
}
