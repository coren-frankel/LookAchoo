{
    "version": 2,
    "builds": [
        {
            "src": "./server/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "./client/build/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server.js"
        }
    ]
}